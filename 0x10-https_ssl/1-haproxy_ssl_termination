defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s

frontend web01.tech
    bind *:443 ssl crt /etc/letsencrypt/live/www.web01.tech/privkey.pem
    reqadd X-Forwarded-Proto:\ https
    default_backend web01.tech

backend web01.tech
    balance roundrobin
    server 247068-web-01 34.224.94.42:80 check
    server 247068-web-02 52.204.18.168:80 check